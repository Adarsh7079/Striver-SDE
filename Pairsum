//5 5
//1 2 3 4 5 
//out put 
//1 4
//2 3

#include <bits/stdc++.h> 
vector<vector<int>> pairSum(vector<int> &v, int t){
    int n=v.size();
     int i=0,j=n-1;
    vector<vector<int>>ans;
    
    int sum=0;
  for(int i=0;i<n;i++)
  {
     for(int j=i+1;j<n;j++)
     {
         if(v[i]+v[j]==t)
         {
             vector<int>temp;
             temp.push_back(min(v[i],v[j]));
             temp.push_back(max(v[i],v[j]));
             ans.push_back(temp);
         }
     }
  }
     sort(ans.begin(),ans.end());
    return ans;
    
    
}
