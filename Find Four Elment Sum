#include <bits/stdc++.h> 
string fourSum(vector<int> v, int t, int n) {
   sort(v.begin(),v.end());
    for(int i=0;i<v.size();i++)
    {
        for(int j=i+1;j<v.size();j++)
        {
            int sum=t-(v[i]+v[j]);
            int p=j+1;
            int q=v.size()-1;
            while(p<q)
            {
                if(v[p]+v[q]==sum)
                {
                    return "Yes";
                }
                else if(v[p]+v[q]>sum)
                {
                    q--;
                }
                else if(v[p]+v[q]<sum)
                {
                    p++;
                }
            }
        }
    }
    return "No";
}
