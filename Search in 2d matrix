#include<iostream>
#include<bits/stdc++.h>
using namespace std;
bool binarysearch(int st,int end,int arr[][3],int m,int k)
{
    int s=0;
    int e=end-1;
    int mid=s+(e-s)/2;
    while(s<=e)
    {
        if(arr[m][mid]==k)
        {
            cout<<mid<<" "<<m<<endl;
            return true;
        }
        else if(arr[m][mid]>k)
        {
            e=mid-1;
        }
        else
        {
            s=mid+1;
        }
        mid=s+(e-s)/2;
    }
    return false;
}
bool find(int arr[][3],int n,int m,int k)
{
    int st=0;
    int e=n-1;
    int mid=st+(e-st)/2;
    while(st<=e)
    {
        if(arr[mid][0]==k)
        {
            cout<<mid<<" "<<"0"<<endl;
            return true;
        }
        if(arr[mid][m-1]==k)
        {
            cout<<mid<<" "<<m-1<<endl;
            return 1;

        }
        if(k>arr[mid][0]&&k<arr[mid][m-1])
        {
            bool ans=binarysearch(n,m,arr,mid,k);
            return 1;
        }
      if(k<arr[mid][0])
      {
          e=mid-1;
      }
      if(k>arr[mid][m-1])
      {
          st=mid+1;
      }
        mid=st+(e-st)/2;
       
    }
    return false;
}
int main()
{
    int n;
    cout<<"Enter Size of aray";
    cin>>n;
    int arr[n][3];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<3;j++)
        {
            cin>>arr[i][j];
        }
    }
    int k;
    cout<<"Enter Key =";
    cin>>k;
    cout<<find(arr,n,3,k)<<endl;
    return 0;
}
