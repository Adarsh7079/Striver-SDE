#include<bits/stdc++.h>
void solve(int index,string s,vector<string>&ans)
{
    if(index>=s.size())
    {
        ans.push_back(s);
        return ;
    }
    for(int i=index;i<s.size();i++)
    {
        swap(s[i],s[index]);
        solve(index+1,s,ans);
         swap(s[i],s[index]);
    }
}
string kthPermutation(int n, int k) {
   vector<string >ans;
    string s;
    for(int i=1;i<=n;i++)
    {
        s.push_back(i+'0');
    }
    solve(0,s,ans);
    sort(ans.begin(),ans.end());
    auto an=ans.begin()+(k-1);
    return *an;
}
