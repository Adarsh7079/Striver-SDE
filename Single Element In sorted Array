int uniqueElement(vector<int> v, int n)
{
    int ans=0;
	if(v[0]!=v[1])
    {
        return v[0];
    }
    else if(v[n-2]!=v[n-1])
    {
        return v[n-1];
    }
    for(int i=1;i<n-1;i++)
    {
        if(v[i]!=v[i+1] && v[i]!=v[i-1])
        {
            ans= v[i];
        }
    }
    return ans;
}
