bool detectCycle(Node *head)
{
	Node* temp1=head;
    Node* temp2=head;
    if(temp1->next==NULL ||temp1==NULL)
    {
        return false;
    }
    while(temp1->next && temp1->next->next)
    {
        temp1=temp1->next->next;
        temp2=temp2->next;
        if(temp1==temp2)
        {
            return true;
        }
    }
    return false;
}
