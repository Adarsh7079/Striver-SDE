  /*  Sample Input 2:

2 
3
4 -5 1
4
1 2 1 -2

Sample Output 2 :

3
0*/

    int LongestSubsetWithZeroSum(vector < int > arr) {
     int n = arr.size();
        int maxLen = 0;
        for(int i=0;i<n ;i++){
            int sum = 0;
            for(int j =i;j<n;j++){
                sum += arr[j];
                if(sum == 0)
                    maxLen = max(maxLen,j-i+1);
            }
        }
        return maxLen;


}
